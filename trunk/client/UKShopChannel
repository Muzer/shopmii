<!--
	var giftTrans = '';
	var titleId = '000100014A424450';
	var transId = giftsArray[titleId];
	
	if ((giftTrans != null) && (giftTrans != "")) {
		// list gifts only
		document.getElementById("giftTitle65").style.display = '';
	} else if (transId != null) {
		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor65").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle65").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle65").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('000100014A424450');
		var latestVersion = parseInt('1');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE65').style.display = ''; 
		}
		var titleLicense = getTitleLicense('000100014A424450');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB65').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB65')[j].onmouseover = 'MM_swapImage("'+ 'Image65' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
