		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor78").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle78").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle78").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('000100014D424C50');
		var latestVersion = parseInt('1');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE78').style.display = ''; 
		}
		var titleLicense = getTitleLicense('000100014D424C50');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB78').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB78')[j].onmouseover = 'MM_swapImage("'+ 'Image78' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA78').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA78')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint78').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint78')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint78')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
