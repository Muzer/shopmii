	} else if (transId != null) {
		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor105").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle105").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle105").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('000100014A414D4D');
		var latestVersion = parseInt('1');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE105').style.display = ''; 
		}
		var titleLicense = getTitleLicense('000100014A414D4D');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB105').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB105')[j].onmouseover = 'MM_swapImage("'+ 'Image105' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA105').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA105')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint105').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint105')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint105')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
