		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor33").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle33").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle33").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('0001000143394850');
		var latestVersion = parseInt('2');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE33').style.display = ''; 
		}
		var titleLicense = getTitleLicense('0001000143394850');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB33').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB33')[j].onmouseover = 'MM_swapImage("'+ 'Image33' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA33').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA33')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint33').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint33')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint33')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
