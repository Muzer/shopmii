			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint168')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint168')[j].innerHTML = 'Downloaded'; 
					}
				}
