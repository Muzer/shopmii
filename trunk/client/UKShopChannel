				}
			}
	
			len=document.getElementsByName('purchasePoint69').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint69')[j].innerHTML = 'Downloadable'; 
