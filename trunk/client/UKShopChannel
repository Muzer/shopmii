		// no pending gift
		document.getElementById("regularTitle222").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('0001000157425150');
		var latestVersion = parseInt('1');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE222').style.display = ''; 
		}
		var titleLicense = getTitleLicense('0001000157425150');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB222').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB222')[j].onmouseover = 'MM_swapImage("'+ 'Image222' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA222').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA222')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint222').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint222')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint222')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
