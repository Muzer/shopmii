	var titleId = '0001000146443250';
	var transId = giftsArray[titleId];
	
	if ((giftTrans != null) && (giftTrans != "")) {
		// list gifts only
		document.getElementById("giftTitle69").style.display = '';
	} else if (transId != null) {
		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor69").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle69").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle69").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('0001000146443250');
		var latestVersion = parseInt('0');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE69').style.display = ''; 
		}
		var titleLicense = getTitleLicense('0001000146443250');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB69').length;
