		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor51").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle51").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle51").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('0001000150423650');
		var latestVersion = parseInt('1');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE51').style.display = ''; 
		}
		var titleLicense = getTitleLicense('0001000150423650');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB51').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB51')[j].onmouseover = 'MM_swapImage("'+ 'Image51' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA51').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA51')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint51').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint51')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint51')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
