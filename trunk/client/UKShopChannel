		document.getElementById("regularTitle60").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('0001000150423350');
		var latestVersion = parseInt('1');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE60').style.display = ''; 
		}
		var titleLicense = getTitleLicense('0001000150423350');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB60').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB60')[j].onmouseover = 'MM_swapImage("'+ 'Image60' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA60').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA60')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint60').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint60')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint60')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
