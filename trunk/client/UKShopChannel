	var giftTrans = '';
	var titleId = '0001000150423850';
	var transId = giftsArray[titleId];
	
	if ((giftTrans != null) && (giftTrans != "")) {
		// list gifts only
		document.getElementById("giftTitle24").style.display = '';
	} else if (transId != null) {
		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor24").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle24").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle24").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('0001000150423850');
		var latestVersion = parseInt('4');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE24').style.display = ''; 
		}
		var titleLicense = getTitleLicense('0001000150423850');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB24').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB24')[j].onmouseover = 'MM_swapImage("'+ 'Image24' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
