		document.getElementById("giftAnchor231").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle231").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle231").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('0001000157425950');
		var latestVersion = parseInt('0');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE231').style.display = ''; 
		}
		var titleLicense = getTitleLicense('0001000157425950');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB231').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB231')[j].onmouseover = 'MM_swapImage("'+ 'Image231' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA231').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA231')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint231').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint231')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint231')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
