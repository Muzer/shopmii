<script type="text/JavaScript">
<!--
	var giftTrans = '';
	var titleId = '0001000146435050';
	var transId = giftsArray[titleId];
	
	if ((giftTrans != null) && (giftTrans != "")) {
		// list gifts only
		document.getElementById("giftTitle20").style.display = '';
	} else if (transId != null) {
		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor20").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle20").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle20").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('0001000146435050');
		var latestVersion = parseInt('1');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE20').style.display = ''; 
		}
		var titleLicense = getTitleLicense('0001000146435050');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB20').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB20')[j].onmouseover = 'MM_swapImage("'+ 'Image20' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA20').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA20')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint20').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint20')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint20')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
