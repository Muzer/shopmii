			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB132')[j].onmouseover = 'MM_swapImage("'+ 'Image132' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA132').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA132')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint132').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint132')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint132')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->

