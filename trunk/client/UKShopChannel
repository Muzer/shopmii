<!--
	var giftTrans = '';
	var titleId = '0001000146414A50';
	var transId = giftsArray[titleId];
	
	if ((giftTrans != null) && (giftTrans != "")) {
		// list gifts only
		document.getElementById("giftTitle110").style.display = '';
	} else if (transId != null) {
		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor110").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle110").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle110").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('0001000146414A50');
		var latestVersion = parseInt('2');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE110').style.display = ''; 
		}
		var titleLicense = getTitleLicense('0001000146414A50');
		if (titleLicense == "Unlimited") {
			// Already own this title
