	var transId = giftsArray[titleId];
	
	if ((giftTrans != null) && (giftTrans != "")) {
		// list gifts only
		document.getElementById("giftTitle96").style.display = '';
	} else if (transId != null) {
		// has pending gift(s). B-24 should be displayed instead of B-05 for the pending gift titles
		document.getElementById("giftAnchor96").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle96").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle96").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('000100014D414E50');
		var latestVersion = parseInt('1');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE96').style.display = ''; 
		}
		var titleLicense = getTitleLicense('000100014D414E50');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB96').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB96')[j].onmouseover = 'MM_swapImage("'+ 'Image96' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA96').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA96')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint96').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint96')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint96')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
