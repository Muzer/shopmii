		document.getElementById("giftAnchor249").href = 'javascript:showGiftTrans('+transId+')';
		document.getElementById("storedGiftTitle249").style.display = '';
	} else {
		// no pending gift
		document.getElementById("regularTitle249").style.display = '';
		var titleInfoVar = ec.getTitleInfo ('000100014A425450');
		var latestVersion = parseInt('1');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE249').style.display = ''; 
		}
		var titleLicense = getTitleLicense('000100014A425450');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB249').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB249')[j].onmouseover = 'MM_swapImage("'+ 'Image249' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA249').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA249')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint249').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint249')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint249')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
