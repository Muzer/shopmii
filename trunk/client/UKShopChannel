		var latestVersion = parseInt('1');
		if (isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
			document.getElementById('UPDATE87').style.display = ''; 
		}
		var titleLicense = getTitleLicense('000100014E414650');
		if (titleLicense == "Unlimited") {
			// Already own this title
			var len=document.getElementsByName('ImageB87').length;
			if (len >0) {
				for(j=0;j<len;j++) {
					document.getElementsByName('ImageB87')[j].onmouseover = 'MM_swapImage("'+ 'Image87' + '","","' + '/common/images/' + '/banner/B_04_softbannerP_b.gif",1); wiiFocusSound();'; 
				}
			}
	
			len=document.getElementsByName('ImageA87').length;
			if (len >0) {
				for(j=0;j<len;j++) {
				document.getElementsByName('ImageA87')[j].src = '/common/images//banner/B_04_softbannerP_a.gif'; 
				}
			}
	
			len=document.getElementsByName('purchasePoint87').length;
			if (!titleInfoVar.isOnDevice || isTitleUpdateAvailable(titleInfoVar, latestVersion)) {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint87')[j].innerHTML = 'Downloadable'; 
					}
				}
			} else {
				if (len >0) {
					for(j=0;j<len;j++) {
						document.getElementsByName('purchasePoint87')[j].innerHTML = 'Downloaded'; 
					}
				}
			}
		}
	}
//-->
</script>

  		
    
    
    
    
